#!/bin/java bsh.Interpreter

source("TestHarness.bsh");

class Foo44 {
	int a=5;
}

class Bar44 {
	int b=5;
	Bar44() {
		b=6; // change value
	}
}

class Gee44 {
	int c=5;
	Gee44( int x ) {
		c=x;
	}
}

foo=new Foo44();
assert(foo.a==5);

bar=new Bar44();
assert(bar.b==6);

assert(isEvalError("new Gee44()")); // no default const
gee=new Gee44(42);
assert(gee.c==42);

complete();

